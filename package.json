{
  "name": "nextjs-boilerplate-app-dir",
  "version": "0.1.0",
  "packageManager": "yarn@4.0.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0 <=18.x.x"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "ts-node": "ts-node -P tsconfig-cjs.json",
    "type-check": "tsc --noEmit",
    "postinstall": "husky install && prisma generate",
    "prisma:db:push": "prisma db push",
    "prisma:db:seed": "prisma db seed",
    "dev": "run-p dev:next dev:contentlayer dev:proxy",
    "dev:next": "next dev",
    "dev:contentlayer": "contentlayer dev",
    "dev:proxy": "local-ssl-proxy --config local-ssl-proxy-config.json",
    "dev:proxy:turbo": "local-ssl-proxy --config local-ssl-proxy-config.json",
    "test": "env-cmd -f ./.env.test run-p lint prettier:check depcruise jest component:headless",
    "test:quick": "run-p lint prettier:check depcruise type-check",
    "build": "run-s build:prisma build:contentlayer build:next",
    "build:next": "next build",
    "build:prisma": "prisma generate",
    "build:contentlayer": "contentlayer build",
    "build:test": "env-cmd -f ./.env.test yarn build",
    "preview": "run-s build start",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write --cache --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "prettier:check": "prettier --cache --check --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "format": "run-s lint:fix prettier",
    "depcruise": "depcruise -v .dependency-cruiser.js ./{app,components,config,hooks,lib,prisma,styles}",
    "jest": "env-cmd -f ./.env.test jest",
    "jest:watch": "env-cmd -f ./.env.test jest --watchAll",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@swc/core": "^1.3.95",
    "@swc/helpers": "^0.5.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.3.3",
    "dependency-cruiser": "^15.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "local-ssl-proxy": "^2.0.5",
    "nock": "^13.3.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "3.0",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "5.5.2",
    "start-server-and-test": "^2.0.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.5.2",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-portal": "^1.0.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@types/mdx": "^2.0.9",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/unist": "^3.0.1",
    "@uidotdev/usehooks": "^2.4.1",
    "@vercel/analytics": "^1.1.1",
    "@vercel/og": "^0.5.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "jose": "^5.0.1",
    "lucide-react": "^0.290.0",
    "mdast-util-toc": "^7.0.0",
    "next": "14.0.0",
    "next-auth": "^4.24.4",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "^9.5.2",
    "react-loader-spinner": "^5.4.5",
    "react-textarea-autosize": "^8.5.3",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-pretty-code": "^0.10.2",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^3.0.1",
    "server-only": "^0.0.1",
    "shiki": "^0.14.5",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "3.3.5",
    "typescript": "^5.2.2",
    "unist-util-visit": "^5.0.0",
    "windy-radix-palette": "^2.0.0-beta.7",
    "windy-radix-typography": "^1.0.0-beta.0",
    "zod": "^3.22.4"
  }
}
