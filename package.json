{
  "name": "nextjs-boilerplate-app-dir",
  "version": "0.1.0",
  "packageManager": "yarn@3.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "npm-run-all -p lint prettier:check jest depcruise component:headless",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "prettier:check": "prettier --check --ignore-path .prettierignore './**/*.{ts,tsx,json,css}'",
    "format": "yarn lint:fix && yarn prettier",
    "depcruise": "depcruise -v .dependency-cruiser.js './src'",
    "jest": "jest",
    "jest:watch": "jest --watchAll",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "cypress": "^12.12.0",
    "dependency-cruiser": "^13.0.0",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "jest": "^29.5.0",
    "nock": "^13.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-color-function": "^4.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/node": "20.1.5",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "next": "13.4.2",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loader-spinner": "^5.3.4",
    "recoil": "^0.7.7",
    "server-only": "^0.0.1",
    "swr": "^2.1.5",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "zod": "^3.21.4"
  }
}
